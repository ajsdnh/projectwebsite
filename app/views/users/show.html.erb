<% provide(:title, @user.name) %>
<div class="page-header">
  <h1>Personal Profile Page</h1>
</div>
<h1>Contact Information</h1>
<div>
<%= gravatar_for @user %>
        <%= @user.name %>
     <%= form_for @user, html: { multipart: true } do |f| %>
    <%= f.file_field :image %>
      <%= image_tag @user.image.url(:medium) %>
</div>
  

  
